{
    "name": "andrey-helldar/cashier-sber-qr",
    "description": "Driver for payment with QR codes via Sber (see andrey-helldar/cashier)",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "bank",
        "billing",
        "cashier",
        "sber",
        "qr"
    ],
    "authors": [
        {
            "name": "Andrey Helldar",
            "email": "helldar@ai-rus.com"
        }
    ],
    "support": {
        "issues": "https://github.com/andrey-helldar/cashier-sber-qr/issues",
        "source": "https://github.com/andrey-helldar/cashier-sber-qr"
    },
    "require": {
        "php": "^7.2.5|^8.0",
        "andrey-helldar/cashier-sber-auth": "dev-main"
    },
    "require-dev": {
        "andrey-helldar/cashier": "dev-main",
        "phpunit/phpunit": "^9.0",
        "symfony/var-dumper": "^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
            "Helldar\\CashierDriver\\Sber\\QR\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
